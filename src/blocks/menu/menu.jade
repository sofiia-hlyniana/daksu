if menuList
	+e("ul").list
		each item in menuList
			- classes = []
				if item.active
					- classes.push("selected")
				if item.color
					- classes.push(item.color)
			+e("li").item(class=classes)
				+e("a").link(href=item.url)
					+e("span").link-icon.icon(data-icon=item.icon)
					+e("span").link-text= item.title
