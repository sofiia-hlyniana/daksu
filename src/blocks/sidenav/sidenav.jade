+e.container
	+e.menu
		+b.menu: +i({ menuList: mainMenu })

	if stacks
		+b.stacks
			+e("h5").title= stacks.title
			+e("ul").list
				each item in stacks.list
					+e("li").item
						+e("span").icon.icon(data-icon="x")
						+e("a").link(href=item.url)= item.title

			+e("button").add-button New Stack
				+e("span").button-icon.icon(data-icon="k")
